{
  "name": "thingengine-server",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "node main.js",
    "lint": "eslint ./routes ./service ./util ./browser",
    "test": "nyc ./tests/everything.sh",
    "update-brassau": "browserify -t babelify ./browser/brassau.js -o public/javascripts/brassau.js",
    "prepare": "(cd node_modules/thingtalk; yarn); (cd node_modules/thingengine-core; yarn; rm -fr node_modules);",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "resolutions": {
    "almond-dialog-agent": "github:stanford-oval/almond-dialog-agent",
    "thingtalk": "github:stanford-oval/thingtalk",
    "thingengine-core": "github:stanford-oval/thingengine-core",
    "thingpedia": "github:stanford-oval/thingpedia-api"
  },
  "dependencies": {
    "almond-dialog-agent": "github:stanford-oval/almond-dialog-agent",
    "babel-core": "^6.26.0",
    "babelify": "^8.0.0",
    "body-parser": "^1.17.2",
    "browserify": "^16.1.1",
    "color-scheme": "^1.0.0",
    "connect-flash": "^0.1.1",
    "cookie-parser": "^1.4.3",
    "csurf": "^1.9.0",
    "dbus-native": "^0.4.0",
    "errorhandler": "^1.5.0",
    "express": "^4.15.3",
    "express-session": "^1.14.0",
    "express-ws": "^3.0.0",
    "gm": "~1.21.1",
    "levelgraph": "^2.0.1",
    "levelup": "^1.3.8",
    "morgan": "^1.8.2",
    "node-gettext": "^2.0.0",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "posix": "^4.1.1",
    "pug": "^2.0.0-rc.2",
    "q": "^1.5.0",
    "serve-favicon": "^2.4.3",
    "smtlib": "^0.1.1",
    "sqlite3": "^4.0.6",
    "thingengine-core": "github:stanford-oval/thingengine-core",
    "thingpedia": "github:stanford-oval/thingpedia-api",
    "thingpedia-discovery": "^1.0.0",
    "thingtalk": "github:stanford-oval/thingtalk",
    "uuid": "^3.1.0",
    "ws": "^3.0.0"
  },
  "optionalDependencies": {
    "node-mimic": "^1.0.0",
    "pulseaudio2": "^0.2.0"
  },
  "devDependencies": {
    "coveralls": "^3.0.6",
    "eslint": "^6.0.0",
    "nyc": "^14.1.1",
    "parse5": "^5.1.0",
    "selenium-webdriver": "^4.0.0-alpha.4"
  },
  "nyc": {
    "exclude": [
      "tests/**",
      "coverage/**"
    ]
  }
}
